{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "exclude": [
    "node_modules/ts-for-gjs/out/index.*"
  ],
  "include": [
    "ts-for-gjs.d.ts",
    "bin",
    "gunit-src"
  ]
}
