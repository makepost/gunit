{
  "name": "gunit",
  "version": "1.0.1",
  "description": "Gets coverage or dies trying. CommonJS runtime for Gjs",
  "main": "gunit-src",
  "types": "gunit-src",
  "bin": {
    "gunit": "bin/coverage.sh"
  },
  "scripts": {
    "coverage": "GJS_COVERAGE_PREFIXES=`pwd`/gunit-src bash bin/coverage.sh",
    "format": "tslint --fix 'bin/**/*.js' 'gunit-src/**/*.js' 'gunit-src/**/*.ts' && tsc",
    "lint": "tslint 'bin/**/*.js' 'gunit-src/**/*.js' 'gunit-src/**/*.ts' && tsc",
    "start": "gjs bin/repl.js",
    "test": "NODE_ENV=production gjs bin/test.js"
  },
  "repository": "https://github.com/makepost/gunit",
  "author": "makepost <makepost@firemail.cc>",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.87",
    "@types/node": "^8.0.54",
    "pre-commit": "^1.2.2",
    "prettier": "^1.12.1",
    "ts-for-gjs": "git+https://github.com/makepost/ts-for-gjs.git",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.6.2"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
